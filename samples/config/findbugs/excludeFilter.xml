<!-- Exclude filter -->
<FindBugsFilter>

    <!-- Match all XYZ violations.
        Dm Internationalization
        EI expose internals
        EI2 storing mutables
        SIC static inner class
        BC Bad Cast
        UrF Unread Field
        HRS Header request splitting
        SE_BAD_FIELD serialization (because of SpringBean annotation in Wicket)
        EQ_UNUSUAL unusual Equals method
        DM_DEFAULT_ENCODING breaks platform portability (running server on windows)
        -->
    <Match>
        <Bug pattern="EQ_UNUSUAL,SE_BAD_FIELD,SE_COMPARATOR_SHOULD_BE_SERIALIZABLE,DM_DEFAULT_ENCODING" code=""/>

    </Match>

    <!-- All bugs in test classes, except for JUnit-specific bugs -->
    <Match>
        <Class name="~.*\.*Test" />
        <Bug pattern="SIC_INNER_SHOULD_BE_STATIC,MS_PKGPROTECT"/>
        <Not>
            <Bug code="IJU"/>
        </Not>
    </Match>

</FindBugsFilter>
